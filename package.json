{
  "name": "moleculer-bun-demo",
  "version": "0.1.1",
  "description": "Moleculer-based microservices project",
  "author": "Rogerio Lucas",
  "type": "module",
  "scripts": {
    "dev": "rimraf dist/; tsup --watch --clean --onSuccess \"SERVICEDIR=dist SERVICES=math moleculer-runner-esm --env --hot --repl\""
